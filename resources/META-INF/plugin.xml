<idea-plugin>
  <id>com.outbina.pogo</id>
  <name>pogo</name>
  <version>1.0.1</version>
  <vendor email="zmgithub@163.com">zmdo</vendor>
  <description>
    In go programming, the plug-in can generate visual interface for persistent object structure of database.
  </description>

  <idea-version since-build="201.0"/>

  <!-- 依赖IDEA平台的插件 -->
  <depends>com.intellij.modules.platform</depends>
  <!-- 依赖go语言插件 -->
  <depends>org.jetbrains.plugins.go</depends>

  <extensions defaultExtensionNs="com.intellij">
    <fileEditorProvider implementation="com.outbina.dbgo.pogo.editor.PogoEditorProvider"/>
    <fileIconProvider implementation="com.outbina.dbgo.pogo.PogoFileIconProvider" order="first"/>
  </extensions>

  <actions>
  </actions>

</idea-plugin>